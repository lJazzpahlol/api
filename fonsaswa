0\n    local bloodP = false\n    local how = 0\n    local totem = false\n    local uhP = false\n    local uhstr = 0\n    local bones = 0\n    aura_env.vir  = 0\n    local uhMight = 0\n    for i=1,aura_env.GetNumBuffs() do \n        -- n, rank, icon, count, debuffType, dur, expirationTime, unitCaster, isStealable, shouldConsolidate, id \n        local name, rank, icon, count, debuffType, expireWhen, expirationTime, unitCaster, isStealable, id, spellId = UnitAura(\"player\", i, \"HELPFUL\")\n        --print(name)\n        --print(id)\n        if name==\"Desolation\" or id == 66803 or id == 63583 or id == 66800 or id == 66801 or id == 66802 then\n            \n            expureDesolation  = expireWhen - GetTime()\n        end\n        \n        if name==\"Blood Presence\" or id ==48266 then\n            --print(id)\n            bloodP  = true\n        end\n        if name==\"Unholy Presence\" or id ==48265 then\n            --print(id)\n            uhP  = true\n        end\n        if name==\"Strength of Earth\" or id ==58646 or id == 10441 or id == 25362 or id == 25527 or id == 57621 then\n            --print(id)\n            totem  = true\n        end\n        \n        if name==\"Horn of Winter\" or id ==57330 or id == 57623 then\n            -- print(id)\n            how  = expireWhen - GetTime()\n        end\n        \n        if name==\"Unholy Strength\" or id ==53365 then\n            --print(id)\n            uhstr  = expireWhen - GetTime()\n        end\n        \n        if name==\"Bone Shield\" or id ==49222 then\n            --print(id)\n            bones  = expireWhen - GetTime()\n        end\n        \n        if name==\"Blood Tap\" or id ==45529 then\n            --print(id)\n            aura_env.BTtimer  = expireWhen - GetTime()\n        end\n        if name==\"Unholy Force\" or id ==67383 then\n            -- print(id)\n            aura_env.vir  = expireWhen - GetTime()\n        end\n        if name==\"Unholy Might\" or id ==67117 then\n            -- print(id)\n            uhMight  = expireWhen - GetTime()\n            if GetTime() - aura_env.BSBuffEnd>=45 then\n                aura_env.BSBuffEnd = 45+GetTime() \n            end\n        end\n    end\n    \n    return expureDesolation, bloodP, how, uhP, uhstr, totem, bones, aura_env.vir\n    \n    \nend\n\naura_env.trackBPet = function() \n    local expureGF = 0\n    if GetSpellCooldown(\"Ghoul Frenzy\")==nil then expureGF=0 end\n    for i=1,aura_env.GetNumBuffsPet() do \n        -- n, rank, icon, count, debuffType, dur, expirationTime, unitCaster, isStealable, shouldConsolidate, id \n        local name, rank, icon, count, debuffType, expireWhen, expirationTime, unitCaster, isStealable, id, spellId = UnitAura(\"pet\", i, \"PLAYER|HELPFUL\")\n        --print(name)\n        if name==\"Ghoul Frenzy\" or id == 63560 then\n            expureGF  = expireWhen - GetTime()\n        end\n    end\n    \n    return expureGF\n    \n    \nend\n\nlocal nameTalent, icon, tier, column, currRank= GetTalentInfo(3,2)\naura_env.DDdur = 30 - (5*currRank)\nlocal nameTalent, icon, tier, column, currRank= GetTalentInfo(3,25)\naura_env.GFtalented = currRank\n\naura_env.DDend = GetTime()-3\naura_env.DDCD = 0\n\n\naura_env.TTD = 180\n\n-- Credit to Aethys https://github.com/herotc/hero-lib\n\nlocal cache = {}\nlocal units = {}\n\nlocal iterableUnits = {\"focus\", \"target\", \"mouseover\"}\nif not WeakAuras.IsClassicEra() then\n    for i=1,5 do\n        iterableUnits[#iterableUnits+1] = \"boss\"..i\n    end\nend\n\nfor i=1,40 do\n    iterableUnits[#iterableUnits+1] = \"nameplate\"..i\nend\n\nfunction aura_env.TTDRefresh()\n    local currentTime = GetTime()\n    local checkedUnits = {}\n    local historyCount = 100\n    local historyTime = 10\n    \n    for _, unit in ipairs(iterableUnits) do\n        if UnitExists(unit) then\n            local GUID = UnitGUID(unit)\n            if not checkedUnits[GUID] then\n                checkedUnits[GUID] = true\n                local health = UnitHealth(unit)\n                local maxHealth = UnitHealthMax(unit)\n                local healthPercentage = health ~= -1 and maxHealth ~= -1 and health / maxHealth * 100\n                -- Check if it's a valid unit\n                if UnitCanAttack(\"player\", unit) and healthPercentage < 100 then\n                    local unitTable = units[GUID]\n                    -- Check if we have seen one time this unit, if we don't then initialize it.\n                    if not unitTable or healthPercentage > unitTable[1][1][2] then\n                        unitTable = { {}, currentTime }\n                        units[GUID] = unitTable\n                    end\n                    local values = unitTable[1]\n                    local time = currentTime - unitTable[2]\n                    -- Check if the % HP changed since the last check (or if there were none)\n                    if #values == 0 or healthPercentage ~= values[1][2] then\n                        local value\n                        local lastIndex = #cache\n                        -- Check if we can re-use a table from the cache -- Buds: i have doubt on the value of reusing table, with the high cost of tinsert on 1st index\n                        if lastIndex == 0 then\n                            value = { time, healthPercentage }\n                        else\n                            value = cache[lastIndex]\n                            cache[lastIndex] = nil\n                            value[1] = time\n                            value[2] = healthPercentage\n                        end\n                        table.insert(values, 1, value)\n                        local n = #values\n                        -- Delete values that are no longer valid\n                        while (n > historyCount) or (time - values[n][1] > historyTime) do\n                            cache[#cache + 1] = values[n]\n                            values[n] = nil\n                            n = n - 1\n                        end\n                    end\n                end\n            end\n        end\n    end\nend\n\nfunction aura_env.TimeToX(guid, percentage, minSamples)\n    --if self:IsDummy() then return 6666 end\n    --if self:IsAPlayer() and Player:CanAttack(self) then return 25 end\n    local seconds = 8888\n    local unitTable = units[guid]\n    -- Simple linear regression\n    -- ( E(x^2)  E(x) )  ( a )  ( E(xy) )\n    -- ( E(x)     n  )  ( b ) = ( E(y)  )\n    -- Format of the above: ( 2x2 Matrix ) * ( 2x1 Vector ) = ( 2x1 Vector )\n    -- Solve to find a and b, satisfying y = a + bx\n    -- Matrix arithmetic has been expanded and solved to make the following operation as fast as possible\n    if unitTable then\n        local values = unitTable[1]\n        local n = #values\n        if n > minSamples then\n            local a, b = 0, 0\n            local Ex2, Ex, Exy, Ey = 0, 0, 0, 0\n            \n            local value, x, y\n            for i = 1, n do\n                value = values[i]\n                x, y = value[1], value[2]\n                \n                Ex2 = Ex2 + x * x\n                Ex = Ex + x\n                Exy = Exy + x * y\n                Ey = Ey + y\n            end\n            -- invariant to find matrix inverse\n            local invariant = 1 / (Ex2 * n - Ex * Ex)\n            -- Solve for a and b\n            a = (-Ex * Exy * invariant) + (Ex2 * Ey * invariant)\n            b = (n * Exy * invariant) - (Ex * Ey * invariant)\n            if b ~= 0 then\n                -- Use best fit line to calculate estimated time to reach target health\n                seconds = (percentage - a) / b\n                -- Subtract current time to obtain \"time remaining\"\n                seconds = math.min(7777, seconds - (GetTime() - unitTable[2]))\n                if seconds < 0 then seconds = 9999 end\n            end\n        end\n    end\n    return seconds\nend\n\n\naura_env.runeSum = 0\nfunction aura_env.runesCD()\n    \n    aura_env.runeSum = 0\n    for i=1, 6 do\n        aura_env.runes[i].cd= aura_env.runes[i].when - GetTime() - aura_env.gcdFreeWhen\n        if  aura_env.runes[i].cd<0 then aura_env.runeSum=aura_env.runeSum+1 end\n        if i>1 then\n            if aura_env.runes[i].cd< -(2.49+aura_env.gcdFreeWhen) then\n                aura_env.runes[i].cd = -2.5\n            end \n        else \n            if GetRuneType(1)~=4 then\n                if aura_env.runes[i].cd< -(2.49+aura_env.gcdFreeWhen) then\n                    aura_env.runes[i].cd = -2.50\n                end \n            end\n        end\n    end\nend\n\n\n\n\naura_env.GCDs = function()\n    local nextRune = 0\n    local m  = 1\n    if aura_env.SStalented==1 then m=0 end\n    local r1 = aura_env.runes[1].cd +m\n    if r1<0 then r1=0 end\n    local r2 = aura_env.runes[2].cd +m\n    if r2<0 then r2=0 end\n    local r3 = aura_env.runes[3].cd\n    if r3<0 then r3=0 end\n    local r4 = aura_env.runes[4].cd\n    if r4<0 then r4=0 end\n    local r5 = aura_env.runes[5].cd +m\n    if r5<0 then r5=0 end\n    local r6 = aura_env.runes[6].cd +m\n    if r6<0 then r6=0 end\n    \n    if r1>r2 then else end\n    local a = 0\n    -- print(\"aura_env.bloodTapLock: \".. tostring(aura_env.bloodTapLock))\n    if aura_env.bloodTapLock then return false else\n        local sum = 0\n        for i=1, 6 do\n            aura_env.runes[i].cd= aura_env.runes[i].when - GetTime() - aura_env.gcdFreeWhen\n            if aura_env.runes[i].cd<1 then\n                sum = sum +1\n            end\n        end\n        \n        \n        if aura_env.nextAction == \"DD\" then\n            \n            a = aura_env.DDwhen+0.5\n        elseif aura_env.nextAction == \"SS\" then\n            a = aura_env.SSlTime\n        elseif aura_env.nextAction == \"IT\" then\n            \n            if r5>r6 then nextRune=r6 else nextRune=r5 end\n            a = nextRune\n            if sum>1 then  a = a - aura_env.fixedGCD*(sum-1) end\n            \n        elseif aura_env.nextAction == \"PS\" or aura_env.nextAction == \"GF\" or aura_env.nextAction == \"PSBS\" or aura_env.nextAction == \"UP\" then\n            \n            if r3>r4 then nextRune=r4 else nextRune=r3 end\n            \n            a = nextRune\n            if sum>1 then  a = a - aura_env.fixedGCD*(sum-1) end\n            \n        elseif aura_env.nextAction == \"BS\" or aura_env.nextAction == \"BB\" or aura_env.nextAction == \"BP\" then\n            \n            if r1>r2 then nextRune=r2 else nextRune=r1 end\n            \n            a = nextRune\n            if sum>1 then  a = a - aura_env.fixedGCD*(sum-1) end\n            \n        else\n            return false\n        end\n    end\n    -- print(\"--\")\n    -- if a < 0 then a = 0 end\n    local b = aura_env.fixedGCD - 0.1\n    --if b<0 then b=0 end\n    \n    -- print(\"time for the next spell to be available: \".. tostring(a))\n    -- print(\"b value:\" .. tostring(b))\n    \n    \n    if a>b then return true else return false end\n    \nend\n\n\naura_env.dndCalc = function(bn, fn, un, bl, fl, ul)\n    \n    -- print(\"bn.bl/fn.fl/un.ul: \"..tostring(aura_env.runes[bn].cd)..\" \"..tostring(aura_env.runes[bl].cd)..\"/\"..tostring(aura_env.runes[fn].cd)..\" \"..tostring(aura_env.runes[fl].cd)..\"/\"..tostring(aura_env.runes[un].cd)..\" \"..tostring(aura_env.runes[ul].cd))\n    \n    local ff = aura_env.ff\n    local uh = aura_env.uh\n    local DESexpire = aura_env.des\n    \n    local timeNow = GetTime()\n    local ffx, uhx, desx, xx = 0,0,0,0\n    local DDend = aura_env.DDend - (timeNow) \n    -- print(DDend) \n    if ff-aura_env.gcdFreeWhen<0.1 then ffx=4 end\n    if uh-aura_env.gcdFreeWhen<0.1 then uhx=5 end\n    -- print(DESexpire)\n    if DESexpire-aura_env.gcdFreeWhen<0.1 then desx=6 end\n    local  SSend = aura_env.SSend - timeNow -aura_env.gcdFreeWhen\n    local ssx = 0\n    if SSend<0.1  and aura_env.SStalented==1 then ssx=1 end\n    \n    local number = 3\n    if aura_env.SStalented==1 then number = 4 end\n    if aura_env.gcdCounter<number and (aura_env.runes[bl].cd<=0)  then  \n        if (DDend < 0) and (ffx or uhx or desx) then \n            -- DDend = 3.5 * aura_env.fixedGCD\n        end\n    elseif aura_env.SStalented==1 and  aura_env.gcdCounter<6 then \n        --if (DDend < 0) and (ffx or uhx or desx or ssxand) then \n        --DDend = 11 * aura_env.fixedGCD\n        -- end\n    end\n    \n    \n    \n    \n    -- print(DDend)\n    aura_env.DDwhen = DDend + (aura_env.config.DDwhen * aura_env.fixedGCD + 0.1) -aura_env.gcdFreeWhen -- + xx*aura_env.fixedGCD\n    local DDwhen = aura_env.DDwhen\n    if DDwhen< -3 then DDwhen = -3 end\n    -- print(\"unedited ddwhen: \".. tostring(DDwhen))\n    local lastCD = 0\n    if DDwhen<0 then \n        local lastRune = 0\n        if aura_env.runes[bn].cd>=aura_env.runes[un].cd and aura_env.runes[bn].cd>= aura_env.runes[fn].cd then \n            lastRune = bn\n        elseif aura_env.runes[un].cd>=aura_env.runes[bn].cd and aura_env.runes[un].cd>= aura_env.runes[fn].cd then \n            lastRune = un\n        elseif aura_env.runes[fn].cd>=aura_env.runes[bn].cd and aura_env.runes[fn].cd>= aura_env.runes[un].cd then \n            lastRune = fn\n        end\n        lastCD = aura_env.runes[lastRune].cd -0.3\n        \n    end\n    if lastCD>1.9 then DDwhen = lastCD  aura_env.DDwhen =DDwhen end\n    \n    if aura_env.TTD-aura_env.gcdFreeWhen < aura_env.config.dndttk then DDwhen = 30 aura_env.DDwhen = 30\n    end\n    \n    \n    \n    local a=0\n    local f = 0\n    f = (15-aura_env.ff+(aura_env.fixedGCD*3)-DDwhen)/(15+(aura_env.fixedGCD*3))\n    local u = 0\n    u = (15-aura_env.uh+(aura_env.fixedGCD*3)-DDwhen)/(15+(aura_env.fixedGCD*3))\n    local d = 0\n    d = (20-aura_env.des+(aura_env.fixedGCD*3)-DDwhen)/(20+(aura_env.fixedGCD*3))\n    local gf = 0\n    gf = (30-aura_env.gf+(aura_env.fixedGCD*3)-DDwhen)/(30+(aura_env.fixedGCD*3))\n    \n    local t = 0\n    if aura_env.impUP==2 then t=3.2\n    elseif aura_env.SStalented==1 then t=1.2    \n    end\n    a = t\n    DDwhen = DDwhen + a\n    aura_env.DDwhen = DDwhen\n    --print(DDwhen)\n    \n    local ddF = 0\n    local ddU = 0\n    local ddB = 0\n    \n    if DDwhen<-3 then DDwhen = -3 end\n    -- print(\"DDwhen: \"..tostring(DDwhen))\n    local cap = 0\n    \n    --print(\"...\")\n    --print(aura_env.runes[bn].cd + 10 - 0.1*aura_env.impUP)\n    local rB, rF, rU = 0,0,0\n    if DDwhen>(aura_env.runes[bl].cd + 10 - 0.1*aura_env.impUP)  then\n        cap = aura_env.runes[bl].cd\n        --if cap<0 then cap =0 end\n        ddB = (cap + 10 - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[bn].cd + 10 - 0.1*aura_env.impUP)  then\n        cap = aura_env.runes[bn].cd\n        --if cap<0 then cap =0 end\n        ddB = (cap + 10 - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[bl].cd)  then\n        cap = aura_env.runes[bl].cd\n        -- if cap<0 then cap =0 end\n        ddB = cap\n        rB = bl\n        \n    else \n        cap = aura_env.runes[bn].cd\n        -- if cap<0 then cap =0 end\n        ddB = cap\n        rB = bn\n    end\n    \n    if DDwhen>(aura_env.runes[fl].cd + 10 - 10*0.05*aura_env.impUP)  then\n        cap = aura_env.runes[fl].cd\n        -- if cap<0 then cap =0 end\n        ddF = (cap+ 10 - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[fn].cd +10 - 10*0.05*aura_env.impUP)  then\n        cap = aura_env.runes[fn].cd\n        -- if cap<0 then cap =0 end\n        ddF = (cap + 10 - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[fl].cd)  then\n        cap = aura_env.runes[fl].cd\n        -- if cap<0 then cap =0 end\n        ddF = (cap)\n        rF =fl\n    else \n        cap = aura_env.runes[fn].cd\n        --  if cap<0 then cap =0 end\n        ddF = (cap)\n        rF =fn\n    end\n    \n    local cap2 = 0\n    cap2 = aura_env.runes[un].cd\n    if cap2<0 then cap2=0 end\n    \n    if DDwhen>(aura_env.runes[ul].cd + 10 - 10*0.05*aura_env.impUP)  then\n        cap = aura_env.runes[ul].cd\n        --if cap<0 then cap =0 end\n        ddU = (cap + (10) - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[un].cd +10 - 10*0.05*aura_env.impUP)  then\n        cap = aura_env.runes[un].cd\n        --if cap<0 then cap =0 end\n        ddU = (cap + (10) - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[ul].cd)  then\n        cap = aura_env.runes[ul].cd\n        -- if cap<0 then cap =0 end\n        ddU = (cap)\n        rU =ul\n    else \n        cap = aura_env.runes[un].cd\n        -- if cap<0 then cap =0 end\n        ddU = (cap)\n        rU =un\n    end\n    \n    \n    \n    if ddB<0 then ddB=0 end\n    if ddF<0 then ddF=0 end\n    if ddU<0 then ddU=0 end\n    \n    return ddB, ddF, ddU, rB, rF, rU\n    \n    \nend\n\n\n\naura_env.runesSS = function(bn, bl, fn, un, fl, ul, B, F, U)--rB, rF, rU\n    -- print(\"B/F/U: \"..tostring(B)..\"/\"..tostring(F)..\"/\"..tostring(U))\n    local fSS = 0\n    local uSS = 0\n    local bSS = 0\n    \n    local SSn = 0\n    local SSl = 0\n    if aura_env.SSend -GetTime()>18 and aura_env.vir<8 then aura_env.SSend=GetTime()+10 end\n    local dndSkip = false\n    if GetTime()-aura_env.ERWused<4 and aura_env.vir<4 then aura_env.SSend=GetTime()-4 dndSkip= true end\n    --print(\"SSend = \"..tostring(aura_env.SSend))\n    local SSwhen = aura_env.SSend -GetTime() -aura_env.gcdFreeWhen\n    if aura_env.vir<18 then SSwhen=SSwhen-2.5 end\n    if aura_env.vir<4 and false then end\n    -- print(\"vir: \"..tostring(aura_env.vir))\n    --print(\"SSend-time: \"..tostring(aura_env.SSend-GetTime()))\n    \n    \n    local bt = aura_env.BTtimer -aura_env.gcdFreeWhen\n    \n    local bnCap = aura_env.runes[bn].cd\n    if bnCap<0 then bnCap=0 end\n    \n    local blCap = aura_env.runes[bl].cd\n    if blCap<0 then blCap=0 end\n    \n    \n    local btRune = 0\n    local btCap = 0\n    if GetRuneType(bn)==4 then\n        btRune = bn\n        btCap = bnCap\n    elseif GetRuneType(bl)==4 then\n        btRune = bl\n        btCap = blCap\n    end\n    \n    local ddB, ddU, ddF = 0,0,0\n    local rB, rU, rF = 0,0,0\n    \n    local DDwhen = SSwhen\n    if DDwhen<-2.6 then DDwhen  = -2.6 end\n    local cap = 0\n    if DDwhen > (aura_env.runes[bl].cd +10 - 10*0.05*aura_env.impUP) and GetRuneType(bl)==4 and bt>(aura_env.runes[bl].cd + 10 - 0.1*aura_env.impUP) then\n        cap = aura_env.runes[bl].cd\n        --if cap<0 then cap =0 end\n        ddB = (cap + 10 - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[bn].cd + 10 - 10*0.05*aura_env.impUP) and GetRuneType(bn)==4 and bt>(aura_env.runes[bn].cd + 10 - 0.1*aura_env.impUP)  then\n        cap = aura_env.runes[bn].cd\n        --if cap<0 then cap =0 end\n        ddB = (cap + 10 - 0.1*aura_env.impUP)\n    elseif (DDwhen>(aura_env.runes[bl].cd)  and GetRuneType(bl)==4 and bt>(aura_env.runes[bl].cd+0.) and (B~=bl or aura_env.gcdCounter<4)) then\n        cap = aura_env.runes[bl].cd\n        -- if cap<0 then cap =0 end\n        ddB = cap\n        rB = bl\n    elseif (GetRuneType(bn)==4 and bt>(aura_env.runes[bn].cd) and (B~=bn or aura_env.gcdCounter<4))   then\n        cap = aura_env.runes[bn].cd\n        -- if cap<0 then cap =0 end\n        ddB = cap\n        rB = bn\n    end\n    if DDwhen<0 then DDwhen  = 0 end\n    local cap = 0\n    if DDwhen>(aura_env.runes[fl].cd +10 - 10*0.05*aura_env.impUP) then\n        cap = aura_env.runes[fl].cd\n        -- if cap<0 then cap =0 end\n        ddF = (cap+ 10 - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[fn].cd +10 - 10*0.05*aura_env.impUP) then\n        cap = aura_env.runes[fn].cd\n        -- if cap<0 then cap =0 end\n        ddF = (cap + 10 - 0.1*aura_env.impUP)\n    elseif (DDwhen>(aura_env.runes[fl].cd) and (F~=fl or aura_env.gcdCounter<4)) and not(dndSkip)   then\n        cap = aura_env.runes[fl].cd\n        -- if cap<0 then cap =0 end\n        ddF = (cap)\n        rF = fl\n    elseif (F~=fn or aura_env.gcdCounter<4 or dndSkip)  then \n        cap = aura_env.runes[fn].cd\n        --  if cap<0 then cap =0 end\n        ddF = (cap)\n        rF = fn\n    end\n    \n    local cap2 = 0\n    cap2 = aura_env.runes[un].cd\n    if cap2<0 then cap2=0 end\n    \n    if DDwhen>(aura_env.runes[ul].cd + 10 - 10*0.05*aura_env.impUP)  then\n        cap = aura_env.runes[ul].cd\n        --if cap<0 then cap =0 end\n        ddU = (cap + (10) - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[un].cd + 10 - 10*0.05*aura_env.impUP)  then\n        cap = aura_env.runes[un].cd\n        --if cap<0 then cap =0 end\n        ddU = (cap + 10 - 0.1*aura_env.impUP)\n    elseif DDwhen>(aura_env.runes[ul].cd) and (U~=ul or aura_env.gcdCounter<4) and not(dndSkip)   then\n        cap = aura_env.runes[ul].cd\n        -- if cap<0 then cap =0 end\n        ddU = (cap)\n        rU = ul\n    elseif (U~=un or aura_env.gcdCounter<4 or dndSkip)  then\n        cap = aura_env.runes[un].cd\n        -- if cap<0 then cap =0 end\n        ddU = (cap)\n        rU = un\n    end\n    \n    \n    if ddB<0 then ddB=0 end\n    if ddF<0 then ddF=0 end\n    if ddU<0 then ddU=0 end\n    \n    local ssnTime, sslTime = 0,0\n    \n    \n    if ddF>ddU then\n        SSn = rU\n        SSl = rF\n        ssnTime = ddU\n        sslTime = ddF\n    else\n        SSn = rF\n        SSl = rU\n        ssnTime = ddF\n        sslTime = ddU\n    end\n    \n    if rB>0 and ddB<ssnTime then\n        SSl = SSn\n        SSn = rb\n    elseif rB>0 and ddB<sslTime then\n        SSl = rb\n    end\n    \n    -- print(\"dndSkip: \"..tostring(dndSkip))\n    --print(\"Inside SSn/SSl: \"..tostring(SSn)..\"/\"..tostring(SSl))\n    \n    aura_env.SSlTime = sslTime\n    \n    --if (SSl==3 or SSl==4) and then \n    \n    -- end\n    \n    return SSn, SSl, sslTime\n    \n    \nend\n\naura_env.ffDT = 0\naura_env.ffTime = 0\naura_env.uhDT = 0\naura_env.uhTime = 0\naura_env.desDT = 0\naura_env.desTime = 0\naura_env.gfDT = 0\naura_env.gfTime = 0\naura_env.vrDT = 0\naura_env.vrTime = 0\naura_env.dndDT = 0\naura_env.dndTime = 0\naura_env.CT = 0\n\n--aura_env.ERW = function() end\n\naura_env.statistics = function(ff, uh, des, gf, dnd, vr)   \n    if UnitAffectingCombat(\"player\") then\n        \n        if  aura_env.CT==0 then\n            aura_env.CT = GetTime()\n            C_Timer.After(\n                30,\n                function()\n                    if (UnitAffectingCombat(\"player\")) then\n                        print(\"\\124cffF772E6 [Fonsas] whispers: I will be reporting statistics for this fight...\\124r\")\n                    end \n                end\n            )\n            \n        end\n        if dnd<=0.1 and aura_env.dndTime==0 then \n            aura_env.dndTime = GetTime()\n        end\n        if aura_env.dndTime>0 and dnd>0.1 then\n            aura_env.dndDT = aura_env.dndDT + GetTime() - aura_env.dndTime\n            aura_env.dndTime = 0\n        end\n        \n        if ff<=0.1 and aura_env.ffTime==0 then \n            aura_env.ffTime = GetTime()\n        end\n        if aura_env.ffTime>0 and ff>0.1 then\n            aura_env.ffDT = aura_env.ffDT + GetTime() - aura_env.ffTime\n            aura_env.ffTime = 0\n        end\n        \n        if uh<=0.1 and aura_env.uhTime==0 then \n            aura_env.uhTime = GetTime()\n        end\n        if aura_env.uhTime>0 and uh>0.1 then\n            aura_env.uhDT = aura_env.uhDT + GetTime() - aura_env.uhTime\n            aura_env.uhTime = 0\n        end\n        \n        if des<=0.1 and aura_env.desTime==0 then \n            aura_env.desTime = GetTime()\n        end\n        if aura_env.desTime>0 and des>0.1 then\n            aura_env.desDT = aura_env.desDT + GetTime() - aura_env.desTime\n            aura_env.desTime = 0\n        end\n        \n        if gf<=0.1 and aura_env.gfTime==0 then \n            aura_env.gfTime = GetTime()\n        end\n        if aura_env.gfTime>0 and gf>0.1 then\n            aura_env.gfDT = aura_env.gfDT + GetTime() - aura_env.gfTime\n            aura_env.gfTime = 0\n        end\n        \n        if vr<=0.1 and aura_env.vrTime==0 then \n            aura_env.vrTime = GetTime()\n        end\n        if aura_env.vrTime>0 and vr>0.1 then\n            aura_env.vrDT = aura_env.vrDT + GetTime() - aura_env.vrTime\n            aura_env.vrTime = 0\n        end\n        \n    else\n        \n        if aura_env.CT>0 and (GetTime()-aura_env.CT)>30 then \n            \n            if aura_env.dndTime>0 then\n                aura_env.dndDT = aura_env.dndDT + GetTime() - aura_env.dndTime\n            end\n            if aura_env.ffTime>0 then\n                aura_env.ffDT = aura_env.ffDT + GetTime() - aura_env.ffTime\n            end\n            if aura_env.uhTime>0 then\n                aura_env.uhDT = aura_env.uhDT + GetTime() - aura_env.uhTime\n            end\n            if aura_env.desTime>0 then\n                aura_env.desDT = aura_env.desDT + GetTime() - aura_env.desTime\n            end\n            if aura_env.gfTime>0 then\n                aura_env.gfDT = aura_env.gfDT + GetTime() - aura_env.gfTime\n            end\n            if aura_env.vrTime>0 then\n                aura_env.vrDT = aura_env.vrDT + GetTime() - aura_env.vrTime\n            end\n            \n            local text = (\"\\124cffF772E6 [Fonsas] whispers: These are the statistics for this fight (uptime %):\\124r\")\n            aura_env.plot(text,1)\n            local num = 100-((aura_env.ffDT)/(GetTime()-aura_env.CT))*100\n            local text = (\"\\124cffF772E6 [Fonsas] whispers: Frost Fever: \"..tostring(tonumber(string.format(\"%.\" .. 1 .. \"f\", num))) ..\"%.\\124r\")\n            aura_env.plot(text,2)\n            local num = 100-((aura_env.uhDT)/(GetTime()-aura_env.CT))*100\n            local text = (\"\\124cffF772E6 [Fonsas] whispers: Blood Plague: \"..tostring(tonumber(string.format(\"%.\" .. 1 .. \"f\", num))) ..\"%.\\124r\")\n            aura_env.plot(text,3)\n            local num = 100-((aura_env.desDT)/(GetTime()-aura_env.CT))*100\n            local text = (\"\\124cffF772E6 [Fonsas] whispers: Desolation: \"..tostring(tonumber(string.format(\"%.\" .. 1 .. \"f\", num))) ..\"%.\\124r\")\n            aura_env.plot(text,4)\n            local num = 100-((aura_env.gfDT)/(GetTime()-aura_env.CT))*100\n            local text = (\"\\124cffF772E6 [Fonsas] whispers: Ghoul Frenzy: \"..tostring(tonumber(string.format(\"%.\" .. 1 .. \"f\", num))) ..\"%.\\124r\")\n            aura_env.plot(text,5)\n            local num = 100-((aura_env.dndDT)/(GetTime()-aura_env.CT))*100\n            local text = (\"\\124cffF772E6 [Fonsas] whispers: Death and Decay: \"..tostring(tonumber(string.format(\"%.\" .. 1 .. \"f\", num))) ..\"%.\\124r\")\n            aura_env.plot(text,6)\n            \n            local i = 7\n            \n            if aura_env.virulenceEquipped then\n                local num = 100-((aura_env.vrDT)/(GetTime()-aura_env.CT))*100\n                local text = (\"\\124cffF772E6 [Fonsas] whispers: Virulence: \"..tostring(tonumber(string.format(\"%.\" .. 1 .. \"f\", num))) ..\"%.\\124r\")\n                aura_env.plot(text,i)\n                i=i+1\n            end\n            \n            local text = (\"\\124cffF772E6 [Fonsas] whispers: You can compare with ideal values by SIM'ing yourself and checking Results> Buffs> Uptime. If you think the NAG could be optimized, please open a bug report on discord.gg/EbonHold\\124r\")\n            aura_env.plot(text,i)\n        end\n        aura_env.ffDT = 0\n        aura_env.ffTime = 0\n        aura_env.uhDT = 0\n        aura_env.uhTime = 0\n        aura_env.desDT = 0\n        aura_env.desTime = 0\n        aura_env.gfDT = 0\n        aura_env.gfTime = 0\n        aura_env.vrDT = 0\n        aura_env.vrTime = 0\n        aura_env.dndDT = 0\n        aura_env.dndTime = 0\n        aura_env.CT = 0\n    end\n    \nend\n\n\naura_env.plot = function(text, time)\n    C_Timer.After(\n        time,\n        function()\n            if not(UnitAffectingCombat(\"player\")) then\n                print(text)\n            end \n        end\n    )\nend\n\naura_env.f1c = 0\naura_env.f2c = 0\naura_env.u1c = 0\naura_env.u2c = 0\naura_env.b1c = 0\naura_env.b2c = 0\n\naura_env.GuitarHero= function(ddB, ddF, ddU, SSn, SSl) \n    if ddF==5 then --fonsas_GH_DND_F1\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_F1\", true)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_F2\", false)\n        \n    elseif ddF==6 then\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_F1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_F2\", true)\n    else \n        WeakAuras.ScanEvents(\"fonsas_GH_DND_F1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_F2\", false)\n    end\n    \n    if ddB==1 then\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_B1\", true)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_B2\", false)\n    elseif ddB==2 then\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_B1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_B2\", true) \n    else\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_B1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_B2\", false)\n    end\n    \n    if ddU==3 then\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_U1\", true)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_U2\", false)\n    elseif ddU==4 then\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_U1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_U2\", true) \n    else\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_U1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_DND_U2\", false)\n    end\n    if (SSn==5 or SSl==5) then --fonsas_GH_SS_F1\n        WeakAuras.ScanEvents(\"fonsas_GH_SS_F1\", true)\n        WeakAuras.ScanEvents(\"fonsas_GH_SS_F2\", false)\n    elseif (SSn==6 or SSl==6) then \n        WeakAuras.ScanEvents(\"fonsas_GH_SS_F1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_SS_F2\", true)\n    else \n        WeakAuras.ScanEvents(\"fonsas_GH_SS_F1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_SS_F2\", false)\n    end\n    \n    \n    if (SSn==3 or SSl==3) then \n        WeakAuras.ScanEvents(\"fonsas_GH_S