eakAuras.ScanEvents(\"fonsas_GH_PES_B1\", false)\n            WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", false)\n        elseif menor>=aura_env.runes[bl].cd+aura_env.gcdFreeWhen then\n            --bn ok// bl pesti\n            if bn==1 then\n                WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", false)\n                WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", true) \n            else\n                WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", true)\n                WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", false) \n            end\n        elseif menor>=aura_env.runes[bn].cd+aura_env.gcdFreeWhen then\n            --bn pesti//bl ok\n            if bn==1 then\n                WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", true)\n                WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", false) \n            else\n                WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", false)\n                WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", true) \n            end\n        else\n            --false false\n            WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", false)\n            WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", false)\n        end\n    else\n        --false false\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", false)\n    end\nend\n\n\n\n",
