.cd>0 then\n        next=un\n    end\n    if aura_env.runes[fn].cd<aura_env.runes[bn].cd and GetRuneType(fn)==4 and aura_env.runes[bn].cd>0 and aura_env.runes[fn].cd<=aura_env.runes[un].cd then\n        next=fn\n    end\n    return next\nend\n\n\naura_env.runesDS = function(fn, un, fl, ul)\n    local fDS = 0\n    local uDS = 0\n    \n    if GetRuneType(un)~=4 then\n        uDS = un\n    elseif GetRuneType(ul)~=4 then\n        uDS = ul\n    end\n    if GetRuneType(fn)~=4 then\n        uDS = un\n    elseif GetRuneType(fl)~=4 then\n        uDS = ul\n    end\n    \n    return fDS, uDS\n    \nend\n\naura_env.bloodGH=function(runePES)\n    if runePES==1 then\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", true)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_F1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_F2\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_U1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_U2\", false)\n    elseif runePES==2 then\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", true)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_F1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_F2\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_U1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_U2\", false)\n    elseif runePES==5 then\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_F1\", true)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_F2\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_U1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_U2\", false)\n    elseif runePES==6 then\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B1\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_B2\", false)\n        WeakAuras.ScanEvents(\"fonsas_GH_PES_F1\", false)\n        WeakAuras.